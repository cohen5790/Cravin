<%- include('../partials/headerCust') %>

    <h1>What You're Cravin</h1>

    <table id="list">
        <tr>
            <th>Name: <br></th>
            <th>Address: <br></th>
            <th >Price: <br></th>
        </tr>
        <tbody>
          <% restaurants.forEach(function(item) { %>
            <tr>
              <td><%= item.name %></td>
              <td><%= item.address %></td>
              <td><% if(item.price === 1){ %>
                    <%= '$' %>
                    <% } %>
                    <% if(item.price === 2){ %>
                        <%= '$$' %>
                        <% } %>
                        <% if(item.price === 3){ %>
                            <%= '$$$' %>
                            <% } %>
                            <% if(item.price === 4){ %>
                                <%= '$$$$' %>
                                <% } %>
                                <% if(item.price === 5){ %>
                                    <%= '$$$$$' %>
                                    <% } %>                
                </td>
              <td><a href="/customer/show/<%= item._id %>">DETAILS</a></td>
            </tr>
            <% }); %>
          </tbody>
    </table><br><br><br><br>




<%- include('../partials/footer') %>